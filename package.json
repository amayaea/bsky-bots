{
  "name": "bsky-bots",
  "version": "1.0.0",
  "main": "dist/index.js",
  "repository": "https://github.com/amayaea/is-virginia-mccaskey-still-alive-bs-bot.git",
  "author": "amayaea <emilio.a.amaya@vanderbilt.edu>",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "clean": "rm -rf node_modules/",
    "start": "ts-node dist/index.js",
    "lint": "prettier --write .",
    "docker:build": "docker compose build",
    "docker:up": "docker compose up -d",
    "docker:down": "docker compose down",
    "docker:deploy": "yarn build && yarn docker:build && yarn docker:up",
    "heroku:up": "heroku ps:scale worker=1",
    "heroku:down": "heroku ps:scale worker=0",
    "gcloud:build": "./scripts/gcloud-build.sh",
    "gcloud:deploy": "./scripts/gcloud-build.sh && ./scripts/gcloud-deploy.sh",
    "prepare": "husky"
  },
  "devDependencies": {
    "@types/node": "^22.9.3",
    "husky": "^9.1.7",
    "lint-staged": "^15.3.0",
    "prettier": "3.4.2",
    "typescript": "^5.7.2"
  },
  "dependencies": {
    "@atproto/api": "^0.13.18",
    "dotenv": "^16.4.5",
    "process": "^0.11.10",
    "ts-node": "^10.9.2",
    "wikipedia": "^2.1.2"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  }
}
